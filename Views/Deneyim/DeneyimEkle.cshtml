@model Mvc5Cv.Models.Entity.TblDeneyimlerim
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>YENİ DENEYİM EKLEME SAYFASI</h4>
<br />
<form method="post" class="form-group">
    <input type="text" name="Baslik" class="form-control" placeholder="FİRMA ADINI YAZINIZ" />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="HANGİ POZİSYONDA ÇALIŞTINIZ ?" />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="TARİH YAZINIZ" />
    <br />
    <textarea name="Aciklama" class="form-control" rows="6" placeholder="DETAYLARI PAYLAŞINIZ" ></textarea>
    <br />
    <button class="btn btn-info">DENEYİM EKLE</button>
</form>

