@model Mvc5Cv.Models.Entity.TblDeneyimlerim
@{
    ViewBag.Title = "DeneyimGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>DENEYİM GÜNCELLEME SAYFASI</h4>
<br />
<form method="post" class="form-group">
    @Html.Label("ID")
    @Html.TextBoxFor(m => m.Id, new { @class = "form-control", @readonly = true })
    @Html.Label("FİRMA ADI")
    @Html.TextBoxFor(m => m.Baslik, new { @class = "form-control" })
    @Html.Label("POZİSYON")
    @Html.TextBoxFor(m => m.AltBaslik, new { @class = "form-control" })
    @Html.Label("TARİH")
    @Html.TextBoxFor(m => m.Tarih, new { @class = "form-control" })
    @Html.Label("AÇIKLAMA")
    @Html.TextAreaFor(m => m.Aciklama, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">DENEYİM GÜNCELLE</button>
</form>
